<h5>Other Items at GPBO</h5>
<table class="striped highlight">
          <thead>
            <th>Name</th>
            <th>Price</th>
          </thead>

        <% for item in @other_items %>
            <tr>
              <td><%= link_to item.name, item_path(item) %></td>
              <td><%= number_to_currency(item.current_retail_price) %></td>
            </tr>
        <% end %>

        
</table><br>